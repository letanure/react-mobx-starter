{
  "name": "react-mobx-starter",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/letanure/react-mobx-starter.git"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "tsc -b && vite build --config vite-bundle-analyzer.config.ts",
    "lint": "tsc --noEmit && biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "type:check": "tsc --noEmit",
    "check:all": "pnpm type:check && pnpm lint && pnpm format:check && pnpm test:unit",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:update:local": "playwright test --update-snapshots",
    "test:e2e:update:ci": "docker run --rm -v $(pwd):/work -w /work -e CI=true mcr.microsoft.com/playwright:v1.54.2-jammy /bin/bash -c 'npm install -g pnpm && pnpm install && pnpm exec playwright test --update-snapshots'",
    "lint:dead-code": "knip || true",
    "lint:dead-code:fix": "knip --fix",
    "size:check": "size-limit",
    "size:why": "size-limit --why",
    "docker:dev": "docker-compose -f docker/docker-compose.yml up app",
    "docker:dev:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:prod": "docker-compose -f docker/docker-compose.yml --profile production up app-prod",
    "docker:build": "docker build -f docker/Dockerfile -t react-starter .",
    "docker:build:dev": "docker build -f docker/Dockerfile.dev -t react-starter:dev .",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "demo:gif": "./scripts/generate-demo-gif.sh desktop",
    "release": "dotenv -- semantic-release",
    "release:dry-run": "dotenv -- semantic-release --dry-run",
    "release:local": "dotenv -- semantic-release --no-ci"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^25.3.2",
    "lucide-react": "^0.536.0",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "motion": "^12.23.12",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.54.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@size-limit/preset-app": "^11.2.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "knip": "^5.62.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "semantic-release": "^24.2.7",
    "size-limit": "^11.2.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css}": [
      "biome check --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "vitest related --run"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
